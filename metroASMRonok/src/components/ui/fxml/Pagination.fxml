<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" spacing="8.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button text="Previous" style="-fx-background-color: transparent; -fx-text-fill: #1f2937; -fx-cursor: hand;" />

        <HBox spacing="4.0">
            <children>
                <Button text="1" style="-fx-background-color: white; -fx-border-color: #e4e4e7; -fx-border-radius: 6;" />
                <Button text="2" style="-fx-background-color: #18181b; -fx-text-fill: white; -fx-background-radius: 6;" />
                <Button text="3" style="-fx-background-color: white; -fx-border-color: #e4e4e7; -fx-border-radius: 6;" />
                <Label text="..." style="-fx-padding: 4 8;" textFill="#71717a"/>
            </children>
        </HBox>

        <Button text="Next" style="-fx-background-color: transparent; -fx-text-fill: #1f2937; -fx-cursor: hand;" />
    </children>
</HBox>