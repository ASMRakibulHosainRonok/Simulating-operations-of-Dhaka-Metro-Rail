<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="16.0" style="-fx-padding: 16; -fx-background-color: white; -fx-border-radius: 8; -fx-border-color: #e4e4e7;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox spacing="4.0">
            <Label text="Radix Primitives" style="-fx-font-weight: bold; -fx-font-size: 14;" />
            <Label text="An open-source UI component library." textFill="#71717a" style="-fx-font-size: 12;" />
        </VBox>

        <Separator orientation="HORIZONTAL" />

        <HBox alignment="CENTER_LEFT" spacing="8.0" style="-fx-pref-height: 20;">
            <Label text="Blog" style="-fx-font-size: 12;" />
            <Separator orientation="VERTICAL" />
            <Label text="Docs" style="-fx-font-size: 12;" />
            <Separator orientation="VERTICAL" />
            <Label text="Source" style="-fx-font-size: 12;" />
        </HBox>
    </children>
</VBox>